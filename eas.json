{
    "cli": {
        "version": ">= 14.0.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "android": {
                "buildType": "apk"
            },
            "channel": "dev",
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "channel": "preview",
            "distribution": "internal"
        },
        "production": {
            "android": {
                "buildType": "app-bundle"
            },
            "channel": "production",
            "distribution": "store",
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {
            "android": {
                "track": "production"
            }
        }
    }
}
